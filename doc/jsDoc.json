{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "source": {
    "include": ["./src/"],
    "exclude": ["./dist/", "./doc/", "./node_modules/", "./test/"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "name": "MesseMap",
    "footerText": "MesseMap",
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "destination": "./doc/",
    "template": "templates/default",
    "encoding": "utf8",
    "readme": "README.md",
    "package": "",
    "access": "all",
    "recurse": true,
    "verbose": true,
    "private": true,
    "pedantic": true
  }
}
